{
  "name": "@hdsydsvenskan/env-sync",
  "version": "1.5.1",
  "license": "MIT",
  "description": "Syncs settings and files across a large number of repositories and services",
  "author": "Pelle Wessman <pelle.wessman@hdsydsvenskan.se>",
  "homepage": "https://github.com/sydsvenskan/node-env-sync",
  "repository": {
    "type": "git",
    "url": "git://github.com/sydsvenskan/node-env-sync"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "envsync": "cli.js"
  },
  "scripts": {
    "check:dependency-check": "dependency-check *.js 'lib/**/*.js' 'test/**/*.js' --no-dev",
    "check:installed-check": "installed-check -e",
    "check:eslint": "eslint .",
    "check": "run-p check:*",
    "test:mocha": "NODE_ENV=test nyc --reporter=lcov --reporter text mocha test/**/*.spec.js",
    "test": "run-s check test:*"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@hdsydsvenskan/eslint-config-hds": "^12.1.0",
    "@types/clone": "^0.1.30",
    "@types/got": "^9.6.5",
    "@types/node": "^12.6.8",
    "chai": "^4.2.0",
    "dependency-check": "^4.1.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-es": "^1.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "husky": "^3.0.2",
    "installed-check": "^2.2.0",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  },
  "dependencies": {
    "@hapi/topo": "^3.1.2",
    "@hdsydsvenskan/plugin-system": "^1.0.0",
    "chalk": "^2.3.0",
    "clone": "^2.1.1",
    "deepmerge": "^3.0.0",
    "fast-deep-equal": "^2.0.0",
    "get-stdin": "^6.0.0",
    "gh-got": "^8.0.0",
    "github-publish": "^3.0.0",
    "got": "^9.0.0",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.10.0",
    "json-diff": "^0.5.2",
    "listr": "^0.14.0",
    "meow": "^5.0.0",
    "strong-ursa": "^0.11.0",
    "util.promisify": "^1.0.0"
  },
  "optionalDependencies": {
    "keytar": "^4.1.0"
  }
}
